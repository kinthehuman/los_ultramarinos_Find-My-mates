<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <KeepRunningUntilFailure>
            <ForceSuccess>
                <Sequence>
                    <Fallback>
                        <Condition ID="ControlData"/>
                        <Sequence>
                            <Action ID="Navegar3"/>
                            <Fallback>
                                <Condition ID="ControlObservador"/>
                                <Action ID="Observar"/>
                            </Fallback>
                            <Action ID="DataCheck"/>
                        </Sequence>
                    </Fallback>
                        <Action ID="Navegar3"/>
                        <Fallback>
                            <Condition ID="ControlDump"/>
                            <Action ID="DataDump"/>
                        </Fallback>
                </Sequence>
            </ForceSuccess>
        </KeepRunningUntilFailure>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
       <Action ID="Observar"/>
       <Action ID="DataCheck"/>
       <Action ID="DataDump"/>
       <Action ID="Navegar3"/>
       <Condition ID="ControlData"/>
       <Condition ID="ControlDump"/>
       <Condition ID="ControlObservador"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

